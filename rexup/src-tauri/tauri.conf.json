{
	"$schema": "https://schema.tauri.app/config/2",
	"productName": "Rexup",
	"version": "1.0.0",
	"identifier": "com.rexup.app",
	"build": {
		"beforeDevCommand": "bun run dev",
		"devUrl": "http://localhost:4321",
		"beforeBuildCommand": "bun run build",
		"frontendDist": "../dist"
	},
	"app": {
		"windows": [
			{
				"title": "Rexup",
				"width": 1600,
				"height": 900
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": ["icons/icon.ico", "icons/icon.png", "icons/icon.svg"]
	}
}
