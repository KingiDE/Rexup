{
	"name": "rexup",
	"private": true,
	"version": "1.0.0",
	"description": "Rexup is a backup creation tool",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"tauri": "tauri",
		"check": "tsc --noEmit --skipLibCheck",
