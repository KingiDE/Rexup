	},
	"dependencies": {
		"@tailwindcss/vite": "^4.0.9",
		"@tauri-apps/api": "^2",
		"@tauri-apps/plugin-opener": "^2",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"tailwindcss": "^4.0.9"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@tauri-apps/cli": "^2",
		"@types/react": "^18.3.1",
		"@types/react-dom": "^18.3.1",
		"@vitejs/plugin-react": "^4.3.4",
		"typescript": "~5.6.2",
		"vite": "^6.0.3"
	}
}
